www-data ALL=(root) NOPASSWD: /usr/bin/cat /etc/kea/kea-dhcp4.conf
www-data ALL=(root) NOPASSWD: /usr/bin/cp /tmp/kea-dhcp4.conf.* /etc/kea/kea-dhcp4.conf
www-data ALL=(root) NOPASSWD: /usr/bin/dd if=* of=/etc/kea/kea-dhcp4.conf bs=1M
www-data ALL=(root) NOPASSWD: /bin/chown _kea:_kea /etc/kea/kea-dhcp4.conf
www-data ALL=(root) NOPASSWD: /bin/chmod 640 /etc/kea/kea-dhcp4.conf
www-data ALL=(root) NOPASSWD: /usr/sbin/kea-dhcp4 -t /etc/kea/kea-dhcp4.conf
www-data ALL=(root) NOPASSWD: /usr/bin/cat /var/lib/kea/kea-leases4.csv

